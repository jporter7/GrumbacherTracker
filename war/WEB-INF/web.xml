<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee">
         


  <!-- Servlets -->
  <servlet>
    <servlet-name>IndexServlet</servlet-name>
    <servlet-class>edu.ycp.cs320.jporter7.servlet.IndexServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>GrumbacherServlet</servlet-name>
    <servlet-class>edu.ycp.cs320.jporter7.servlet.GrumbacherServlet</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>RacquetBallServlet</servlet-name>
    <servlet-class>edu.ycp.cs320.jporter7.servlet.RacquetBallServlet</servlet-class>
  </servlet>
  
   <servlet>
    <servlet-name>WeightRoomServlet</servlet-name>
    <servlet-class>edu.ycp.cs320.jporter7.servlet.WeightRoomServlet</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>AccountCreationServlet</servlet-name>
    <servlet-class>edu.ycp.cs320.jporter7.servlet.AccountCreationServlet</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>ReservationsServlet</servlet-name>
    <servlet-class>edu.ycp.cs320.jporter7.servlet.ReservationsServlet</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>edu.ycp.cs320.jporter7.servlet.LoginServlet</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>PoolServlet</servlet-name>
    <servlet-class>edu.ycp.cs320.jporter7.servlet.PoolServlet</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>FieldHouseServlet</servlet-name>
    <servlet-class>edu.ycp.cs320.jporter7.servlet.FieldHouseServlet</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>WolfBasketballServlet</servlet-name>
    <servlet-class>edu.ycp.cs320.jporter7.servlet.WolfBasketballServlet</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>RockWallServlet</servlet-name>
    <servlet-class>edu.ycp.cs320.jporter7.servlet.RockWallServlet</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>DanceRoomServlet</servlet-name>
    <servlet-class>edu.ycp.cs320.jporter7.servlet.DanceRoomServlet</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>UserProfileServlet</servlet-name>
    <servlet-class>edu.ycp.cs320.jporter7.servlet.UserProfileServlet</servlet-class>
  </servlet>
  
  <!-- AJAX servlets. -->
  <servlet>
    <servlet-name>RacquetBallAjaxServlet</servlet-name>
    <servlet-class>edu.ycp.cs320.jporter7.servlet.ajax.RacquetBallAjaxServlet</servlet-class>
  </servlet>  
  
  <servlet>
    <servlet-name>WeightRoomAjaxServlet</servlet-name>
    <servlet-class>edu.ycp.cs320.jporter7.servlet.ajax.WeightRoomAjaxServlet</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>AccountCreationAjaxServlet</servlet-name>
    <servlet-class>edu.ycp.cs320.jporter7.servlet.ajax.AccountCreationAjaxServlet</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>ReservationsAjaxServlet</servlet-name>
    <servlet-class>edu.ycp.cs320.jporter7.servlet.ajax.ReservationsAjaxServlet</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>LoginAjaxServlet</servlet-name>
    <servlet-class>edu.ycp.cs320.jporter7.servlet.ajax.LoginAjaxServlet</servlet-class>
  </servlet>
	
  <!-- Servlet mappings -->
  <servlet-mapping>
    <servlet-name>IndexServlet</servlet-name>
    <url-pattern>/index</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GrumbacherServlet</servlet-name>
    <url-pattern>/grumbacher</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>RacquetBallServlet</servlet-name>
    <url-pattern>/racquetBall</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>WeightRoomServlet</servlet-name>
    <url-pattern>/weightRoom</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>AccountCreationServlet</servlet-name>
    <url-pattern>/accountCreation</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>ReservationsServlet</servlet-name>
    <url-pattern>/reservations</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>PoolServlet</servlet-name>
    <url-pattern>/pool</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>FieldHouseServlet</servlet-name>
    <url-pattern>/fieldHouse</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>WolfBasketballServlet</servlet-name>
    <url-pattern>/wolfBasketball</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>RockWallServlet</servlet-name>
    <url-pattern>/rockWall</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>DanceRoomServlet</servlet-name>
    <url-pattern>/danceRoom</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>UserProfileServlet</servlet-name>
    <url-pattern>/userProfile</url-pattern>
  </servlet-mapping>
  
  <!--  AJAX servlet mappings. -->
  <servlet-mapping>
    <servlet-name>RacquetBallAjaxServlet</servlet-name>
    <url-pattern>/ajax/racquetBall</url-pattern>
  </servlet-mapping>

  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  
  <servlet-mapping>
    <servlet-name>WeightRoomAjaxServlet</servlet-name>
    <url-pattern>/ajax/weightRoom</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>AccountCreationAjaxServlet</servlet-name>
    <url-pattern>/ajax/accountCreation</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>ReservationsAjaxServlet</servlet-name>
    <url-pattern>/ajax/reservations</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>LoginAjaxServlet</servlet-name>
    <url-pattern>/ajax/login</url-pattern>
  </servlet-mapping>
  
</web-app>
